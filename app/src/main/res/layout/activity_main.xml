<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MenuActivity"
    android:orientation="vertical">

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <ImageView
            android:layout_width="180dp"
            android:layout_height="244dp"
            android:id="@+id/imageView3"
            android:src="@drawable/miky" />

        <ImageView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/imageView2"
            android:src="@drawable/rn" />

    </LinearLayout>

    <Button
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Mapa"
        android:id="@+id/bMap"
        android:onClick="Map" />

    <Button
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Lista obiektÃ³w"
        android:id="@+id/bList"
        android:onClick="Lists" />

    <Button
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Ustawienia"
        android:id="@+id/bUstawienia"
        android:onClick="Settings" />

    <Button
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Pomoc"
        android:id="@+id/bAbout"
        android:onClick="Help" />
</LinearLayout>